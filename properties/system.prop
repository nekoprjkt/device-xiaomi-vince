# ART/Dex2oat
ro.sys.fw.dex2oat_thread_count=8

# Boot
sys.vendor.shutdown.waittime=500

# Camera
persist.vendor.camera.display.lmax=1280x720
persist.vendor.camera.display.umax=1920x1080
vendor.camera.lowpower.record.enable=1
media.camera.ts.monotonic=1
persist.camera.mct.debug=0
persist.camera.iface.logs=0
persist.camera.imglib.logs=0
persist.camera.isp.clock.optmz=0
persist.camera.isp.debug=0
persist.camera.sensor.debug=0
persist.camera.stats.test=5
vidc.enc.dcvs.extra-buff-count=2

# Display/Graphics
persist.hwc.enable_vds=1
persist.hwc.mdpcomp.enable=true
persist.hwc.ptor.enable=true
sdm.debug.disable_skip_validate=1

# Fingerprint
persist.qfp=false

# FM
ro.fm.transmitter=false

# Frp
ro.frp.pst=/dev/block/bootdevice/by-name/config

# Media
av.debug.disable.pers.cache=1
media.msm8956hw=0
media.stagefright.audio.sink=280
mmp.enable.3g2=true
vendor.video.disable.ubwc=1

# codecs:(PARSER_)AAC AC3 ASF FLV 3G2 MP2PS MP2TS QCP AIFF
vendor.mm.enable.qcom_parser=301715

# Disable media transcoding
persist.sys.fuse.transcode_user_control=true
persist.sys.fuse.transcode_enabled=false

# Network manager daemon
persist.data.netmgrd.qos.enable=true

# Radio/Telephony
persist.vendor.ims.disableIMSLogs=1
persist.vendor.ims.disableADBLogs=1
persist.vendor.ims.disableDebugDataPathLogs=1
persist.vendor.ims.disableDebugLogs=1
persist.vendor.ims.disableSigHandler=1
persist.vendor.ims.disableQXDMLogs=1
ril.subscription.types=NV,RUIM

# Recovery
persist.sys.recovery_update=false

# System
persist.sys.binary_xml=false

# Tcp
net.tcp.2g_init_rwnd=10

# Touch
ro.sf.delayedtouches=0
touch.orientationAware=1
SurfaceOrientation=auto
touch.size.calibration=auto
touch.size.scale=auto
touch.size.isSummed=1
persist.device_config.runtime_native.usap_pool_enabled=true
touch.orientation.calibration=auto
touch.distance.calibration=auto
touch.deviceType=auto
touch.distance.scale=auto
touch.coverage.calibration=octagram
touch.pressure.scale=auto
touch.gesturemode=spots
MultitouchMinDistance=auto
MultitouchSettleInterval=0ms
TapInterval=0ms
QuietInterval=0ms
MovementSpeedRatio=auto
TapSlop=0px
persist.dev.pm.dyn_samplingrate=9999999999999999999999999999
touch.pressure.calibration=auto
scroll.accelerated.hw=true
persist.sys.ui.hwframes=9999999999999999999999999999
persist.sys.force_high_end_gfx=1
view.scroll_friction=auto
ro.min_pointer_dur=auto
ro.sf.disable_smooth_effect=true
ro.product.multi_touch_enabled=true
ro.product.max_num_touch=auto
ro.window.animation.scale=0.88
ro.transition.animation.scale=0.88
view.touch_slop=0dp
windowsmgr.maxeventspersec=9999999999999999999999999999

# Watchdog
PRODUCT_SYSTEM_PROPERTIES +=
ro.hw_timeout_multiplier=3

# Wifi Display (Platform)
media.wfd.max_resolution=5

# Zygote
zygote.critical_window.minute=10
