# AOSP FM
allow system_app fm_radio_device:chr_file r_file_perms;

# Bluetooth
get_prop(system_app, vendor_bluetooth_prop)

# XiaomiParts
allow system_app {
    sysfs_hgain
    sysfs_mgain
    sysfs_sgain
}:file rw_file_perms;

# zram
